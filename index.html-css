em css

*{
  margin: 0;
  padding: 0;
  border: 0;
}
header{
width: 100%;
height: 200px;
background-image: linear-gradient(#2196f3, #305afe, #0d47a1);

}
h1 {
  margin: 0 auto;
  color: #fff;
  display: flex;
  justify-content: center;
  padding: 10px;
  text-align: center;
}
h2{
  margin-left: 80px;
  color: #fff;
  display: flex;
  justify-content: center;
  text-align: center;
}

form{
  margin-top: 100px;
  margin-left: 250px;
  width: 45%;
  font-family: Arial, Helvetica, sans-serif;
  border: 1px solid black;
  padding: 20px;
}
form div + div{
  margin-top: 20px;

}
label{
  display: inline-block;
  width: 90px;
  text-align:center;
}
input{
  width: 250px;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
  border: 1px solid black;
  padding: 10px;
}
.estilo{
  padding-left: 90px;
  margin-left: 95px;
  color:#fff;
  padding: 10px;
  background-color:#305afe ;
  border-radius: 5px;
  border-bottom: 2px solid #000;

}
 .material-icons{
  width: 0 auto;
  margin-left: 410px;
  margin-top: 50px ;
  padding: 25px;
  border-radius: 100%;
  background-color: #fff;
  font-size: 100px;
  color: #305afe;

}


